<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE struts PUBLIC  
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"  
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>	
	<package name="struts2" extends="struts-default" namespace="/">
	
		<result-types>
			<result-type name="jsonResult" class="com.lifedjtu.jw.ui.struts2.result.JsonResult"></result-type>
		</result-types>
	
		<action name="index" class="com.lifedjtu.jw.ui.struts2.core.MainAction">
			<result name="success">/WEB-INF/jsp/index.jsp</result>
		</action>
		
		<action name="signin" method="signin" class="com.lifedjtu.jw.ui.struts2.core.MainWebServiceAction">
			<result name="success" type="jsonResult">message</result>
		</action>
		
		
	</package>
</struts>